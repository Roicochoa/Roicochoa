def isPalindrome(x: int) -> bool: #palindromes for integers
    
    number = x
    temp = x
    reverse_num=0
       
    while(number>0):
            digit = number%10
            reverse_num = reverse_num*10 + digit
            number = number//10
        
    if(temp == reverse_num):
            return True
        
    else:
            return False
